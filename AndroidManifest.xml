                <action android:name="com.igexin.sdk.action.p7xKS7Oohd7XSd0ZU51X03" />
            </intent-filter>
        </receiver>

        <!-- Receiver -->
        <provider
            android:name="com.igexin.download.DownloadProvider"
            android:authorities="downloads.com.android.goodhelpercarair"
            android:process=":pushservice" />
        <!-- android:authorities="downloads.第三方包名" -->
