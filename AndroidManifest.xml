                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
        </receiver>
        <!-- Receiver -->
        <receiver
            android:name="com.android.carair.receiver.CarairPushReceiver"
            android:exported="false" >
            <intent-filter>
